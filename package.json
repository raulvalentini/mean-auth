    "@angular/router": "^7.1.3",
    "@auth0/angular-jwt": "^2.1.0",
    "@ng-bootstrap/ng-bootstrap": "^4.0.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.1.3",
    "cookie-parser": "^1.4.0",
    "core-js": "^2.6.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.4",
    "express-session": "^1.12.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.4.0",
    "ldapjs": "^1.0.2",
    "lodash": "^4.17.11",
    "mongoose": "^5.4.4",
